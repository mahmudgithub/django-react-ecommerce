 </thead>\r\n                <tbody>\r\n                    <tr>\r\n                        {\r\n                            details != null && (\r\n                                <>\r\n                                    <td>{details.date}</td>\r\n                                    <td>{details.total}</td>\r\n                                    <td>{details.email}</td>\r\n                                    <td>{details.mobile}</td>\r\n                                    <td>{details.discount}</td>\r\n                                    <td>{details.cartproduct?.length}</td>\r\n                                </>\r\n                            )\r\n                        }\r\n                    </tr>\r\n                </tbody>\r\n            </table>\r\n            <h1>Product details</h1>\r\n            <table className=\"table table-bordered \">\r\n                <thead>\r\n                    <tr>\r\n                        <th>SN</th>\r\n                        <th>Product</th>\r\n                        <th>Price</th>\r\n                        <th>Quantity</th>\r\n                        <th>Subtotal</th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                    {\r\n                        products?.map((data, i) => (\r\n                            <tr key={i}>\r\n                                <td>{i + 1}</td>\r\n                                <td>{data.product[0].title}</td>\r\n                                <td>{data.price}</td>\r\n                                <td>{data.quantity}</td>\r\n                                <td>{data.subtotal}</td>\r\n                            </tr>\r\n                        ))\r\n                    }\r\n                </tbody>\r\n            </table>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default OldordersDetails\r\n","C:\\Users\\SK\\Desktop\\single_page_Ecommerce\\react_ecommerce\\src\\components\\Order.jsx",[],"C:\\Users\\SK\\Desktop\\single_page_Ecomm